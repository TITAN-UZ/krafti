{
  "name": "krafti",
  "version": "1.0.0",
  "description": "Krafti website",
  "author": "Vasily Naumkin",
  "private": true,
  "scripts": {
    "analyze:admin": "nuxt build --analyze --config-file src/admin/nuxt.config.js",
    "analyze:site": "nuxt build --analyze --config-file src/site/nuxt.config.js",
    "dev:admin": "nuxt --config-file src/admin/nuxt.config.js -p=4000 -H=0.0.0.0",
    "dev:site": "nuxt --config-file src/site/nuxt.config.js -p=4100 -H=0.0.0.0",
    "build:admin": "nuxt build --config-file src/admin/nuxt.config.js",
    "build:site": "nuxt build --config-file src/site/nuxt.config.js",
    "generate:admin": "nuxt generate --config-file src/admin/nuxt.config.js",
    "generate:site": "nuxt generate --config-file src/site/nuxt.config.js",
    "start": "yarn build:site && pm2 start --env production && sleep 5 && chmod 666 ../tmp/nuxt.socket",
    "stop": "pm2 del all",
    "restart": "yarn build:site && pm2 restart all && sleep 5 && chmod 666 ../tmp/nuxt.socket",
    "analyze": "concurrently 'yarn analyze:admin' 'yarn analyze:site'",
    "dev": "concurrently 'yarn dev:admin' 'yarn dev:site'",
    "build": "concurrently 'yarn build:admin' 'yarn build:site'",
    "generate": "concurrently 'yarn generate:admin' 'yarn generate:site'",
    "lint": "eslint --ext .js,.vue --ignore-path ../.gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path ../.gitignore ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.9.0",
    "@fortawesome/pro-duotone-svg-icons": "^5.10.0",
    "@fortawesome/pro-light-svg-icons": "^5.9.0",
    "@fortawesome/pro-regular-svg-icons": "^5.9.0",
    "@fortawesome/pro-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@nuxtjs/auth": "^4.8.1",
    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/markdownit": "^1.2.6",
    "@nuxtjs/moment": "^1.2.0",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/sitemap": "^1.3.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "apexcharts": "^3.22.3",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "bootswatch": "^4.4.1",
    "concurrently": "^5.1.0",
    "filepond": "^4.4.11",
    "filepond-plugin-file-encode": "^2.1.4",
    "filepond-plugin-file-validate-type": "^1.2.4",
    "filepond-plugin-image-crop": "^2.0.3",
    "filepond-plugin-image-exif-orientation": "^1.0.6",
    "filepond-plugin-image-preview": "^4.2.1",
    "filepond-plugin-image-resize": "^2.0.4",
    "filepond-plugin-image-transform": "^3.3.2",
    "intersection-observer": "^0.9.0",
    "node-sass": "^4.12.0",
    "nuxt": "^2.14.10",
    "nuxt-facebook-pixel-module": "^1.5.0",
    "nuxt-izitoast": "^0.1.2",
    "plural-ru": "^2.0.1",
    "pm2": "^3.5.1",
    "responsive-loader": "^1.2.0",
    "sass-loader": "^7.1.0",
    "sharp": "^0.23.0",
    "smart-truncate": "^1.0.1",
    "vue-analytics": "^5.17.2",
    "vue-apexcharts": "^1.5.3",
    "vue-autosuggest": "^2.0.2",
    "vue-carousel": "^0.18.0",
    "vue-clipboard2": "^0.3.0",
    "vue-filepond": "^5.1.2",
    "vue-gallery": "^2.0.1",
    "vue-masonry": "^0.11.8",
    "vue-social-sharing": "^2.4.6",
    "vue-vimeo-player": "^0.0.9",
    "vue-yandex-metrika": "^1.8.2",
    "vue2-datepicker": "^3.8.2",
    "vuedraggable": "^2.23.0",
    "yarn": "^1.17.3"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "^1.19.1"
  }
}
